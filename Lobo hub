    StatsLabel.Size = UDim2.new(1, -30, 0, 130)
    StatsLabel.Font = Enum.Font.Gotham
    StatsLabel.Text = "Carregando estatÃ­sticas..."
    StatsLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    StatsLabel.TextSize = 13
    StatsLabel.TextXAlignment = Enum.TextXAlignment.Left
    StatsLabel.TextYAlignment = Enum.TextYAlignment.Top
    
    ToggleButton.Name = "ToggleButton"
    ToggleButton.Parent = MainFrame
    ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
    ToggleButton.Position = UDim2.new(0, 15, 0, 230)
    ToggleButton.Size = UDim2.new(1, -30, 0, 35)
    ToggleButton.Font = Enum.Font.GothamBold
    ToggleButton.Text = "â–¶ INICIAR FARM"
    ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleButton.TextSize = 16
    
    UICorner2.CornerRadius = UDim.new(0, 8)
    UICorner2.Parent = ToggleButton
    
    ToggleButton.MouseButton1Click:Connect(function()
        LBHub.IsRunning = not LBHub.IsRunning
        if LBHub.IsRunning then
            ToggleButton.Text = "â¸ PAUSAR FARM"
            ToggleButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
            LBHub:Notify("Iniciado!", "Farm ativado com sucesso!", 3)
        else
            ToggleButton.Text = "â–¶ INICIAR FARM"
            ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
            LBHub:Notify("Pausado!", "Farm pausado.", 3)
        end
    end)
    
    -- Update Loop
    spawn(function()
        while wait(1) do
            pcall(function()
                LBHub.Stats.Runtime = tick() - LBHub.Stats.StartTime
                
                StatusLabel.Text = "Status: " .. LBHub.CurrentTask
                StatusLabel.TextColor3 = LBHub.IsRunning and Color3.fromRGB(0, 255, 150) or Color3.fromRGB(255, 150, 0)
                
                local hours = math.floor(LBHub.Stats.Runtime / 3600)
                local minutes = math.floor((LBHub.Stats.Runtime % 3600) / 60)
                local seconds = math.floor(LBHub.Stats.Runtime % 60)
                
                StatsLabel.Text = string.format(
                    "ğŸ“Š ESTATÃSTICAS:\n\n" ..
                    "Level: %d\n" ..
                    "Beli: %s\n" ..
                    "Fragmentos: %d\n" ..
                    "Quests: %d\n" ..
                    "Bosses: %d\n" ..
                    "Frutas: %d\n" ..
                    "Tempo: %02d:%02d:%02d",
                    Player.Data.Level.Value,
                    tostring(Player.Data.Beli.Value),
                    Player.Data.Fragments.Value,
                    LBHub.Stats.QuestsCompleted,
                    LBHub.Stats.BossesDefeated,
                    LBHub.Stats.FruitsCollected,
                    hours, minutes, seconds
                )
            end)
        end
    end)
end

