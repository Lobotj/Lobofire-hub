    warn("âš ï¸ LoboFire Hub jÃ¡ estÃ¡ em execuÃ§Ã£o!")
    return
end
getgenv().LoboFireLoaded = true

-- NotificaÃ§Ã£o de carregamento
local function Notify(title, text, duration)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = title,
        Text = text,
        Duration = duration or 5,
        Icon = "rbxassetid://0"
    })
end

Notify("ðŸ”¥ LoboFire Hub", "Iniciando carregamento...", 3)

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- MÃ“DULO: SISTEMA ANTI-BAN AVANÃ‡ADO
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local AntiBan = {}
AntiBan.__index = AntiBan

AntiBan.Config = {
    MinDelay = 0.5,
    MaxDelay = 2.5,
    ActionDelayMin = 1.2,
    ActionDelayMax = 3.8,
    HumanReactionTime = {0.15, 0.45},
    ServerHopOnModerator = true,
    ServerHopOnSuspicious = true,
    MaxPlayersInServer = 12,
    AntiKick = true,
    AntiAfk = true,
    RandomMovement = true,
    CameraRandomization = true,
}

AntiBan.LastAction = tick()
AntiBan.ActionCount = 0

function AntiBan:RandomDelay(min, max)
    min = min or self.Config.MinDelay
    max = max or self.Config.MaxDelay
    local delay = min + (math.random() * (max - min))
    delay = delay + (math.random(-100, 100) / 1000)
    return delay
end

function AntiBan:Wait(customMin, customMax)
    local delay = self:RandomDelay(customMin, customMax)
    task.wait(delay)
    self.LastAction = tick()
    self.ActionCount = self.ActionCount + 1
end

function AntiBan:HumanReaction()
    local reactionTime = self.Config.HumanReactionTime
    local delay = reactionTime[1] + (math.random() * (reactionTime[2] - reactionTime[1]))
    task.wait(delay)
end

function AntiBan:DetectModerator()
    local Players = game:GetService("Players")
    local moderatorTags = {"Moderator", "Admin", "Staff", "[MOD]", "[ADMIN]"}
    
    for _, player in pairs(Players:GetPlayers()) do
        local displayName = player.DisplayName or ""
        local userName = player.Name or ""
        
        for _, tag in pairs(moderatorTags) do
            if string.find(displayName:lower(), tag:lower()) or 
               string.find(userName:lower(), tag:lower()) then
                return true, player.Name
            end
        end
    end
    
    return false, nil
end

function AntiBan:ServerHop()
    local TeleportService = game:GetService("TeleportService")
    local Players = game:GetService("Players")
    local PlaceId = game.PlaceId
    
    pcall(function()
        TeleportService:Teleport(PlaceId, Players.LocalPlayer)
    end)
end

function AntiBan:EnableAntiKick()
    if not self.Config.AntiKick then return end
    
    local success = pcall(function()
        local mt = getrawmetatable(game)
        local old = mt.__namecall
        setreadonly(mt, false)
        
        mt.__namecall = newcclosure(function(self, ...)
            local method = getnamecallmethod()
            if method == "Kick" then
                return nil
            end
            return old(self, ...)
        end)
        
        setreadonly(mt, true)
    end)
    
    if success then
        print("âœ“ Anti-Kick ativado")
    end
end

function AntiBan:EnableAntiAfk()
    if not self.Config.AntiAfk then return end
    
    local VirtualUser = game:GetService("VirtualUser")
    
    game:GetService("Players").LocalPlayer.Idled:Connect(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end)
    
    print("âœ“ Anti-AFK ativado")
end

function AntiBan:StartMonitoring()
    spawn(function()
        while task.wait(30) do
            local hasMod, modName = self:DetectModerator()
            if hasMod and self.Config.ServerHopOnModerator then
                warn("âš ï¸ Moderador detectado:", modName)
                Notify("âš ï¸ Alerta", "Moderador detectado! Mudando servidor...", 3)
                task.wait(2)
                self:ServerHop()
            end
            
            if tick() - self.LastAction > 120 then
                self.ActionCount = 0
            end
        end
    end)
end

function AntiBan:Initialize()
    self:EnableAntiKick()
    self:EnableAntiAfk()
    self:StartMonitoring()
    print("ðŸ”’ Sistema Anti-Ban LoboFire ativado!")
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- MÃ“DULO: SISTEMA DE FARM OTIMIZADO
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local Farm = {}
Farm.__index = Farm

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

local Player = Players.LocalPlayer

Farm.Config = {
    Enabled = false,
    FarmLevel = true,
    FarmMastery = false,
    FarmBoss = false,
    FarmMaterial = false,
    AttackDistance = 25,
    SafeDistance = 30,
    FastAttack = true,
    AutoHaki = true,
    DisableRendering = true,
    RemoveEffects = true,
    BoostFPS = true,
}

Farm.Status = {
    CurrentTask = "Inicializando...",
    CurrentMob = "Nenhum",
    Level = 0,
    QuestProgress = "0/0",
    TimeElapsed = 0,
    BossesDefeated = 0,
    ElitesDefeated = 0,
}

Farm.MobList = {
    {Level = 1, Name = "Bandit", Quest = "BanditQuest1"},
    {Level = 10, Name = "Monkey", Quest = "JungleQuest"},
    {Level = 15, Name = "Gorilla", Quest = "JungleQuest"},
    {Level = 20, Name = "Pirate", Quest = "BuggyQuest1"},
    {Level = 30, Name = "Brute", Quest = "BuggyQuest1"},
    {Level = 40, Name = "Desert Bandit", Quest = "DesertQuest"},
    {Level = 60, Name = "Desert Officer", Quest = "DesertQuest"},
    {Level = 75, Name = "Snow Bandit", Quest = "SnowQuest"},
    {Level = 90, Name = "Snowman", Quest = "SnowQuest"},
    {Level = 100, Name = "Chief Petty Officer", Quest = "MarineQuest2"},
    {Level = 120, Name = "Sky Bandit", Quest = "SkyQuest"},
    {Level = 150, Name = "Dark Master", Quest = "SkyQuest"},
    {Level = 700, Name = "Raider", Quest = "Area1Quest"},
    {Level = 725, Name = "Mercenary", Quest = "Area1Quest"},
    {Level = 750, Name = "Swan Pirate", Quest = "Area2Quest"},
    {Level = 1500, Name = "Pirate Millionaire", Quest = "PiratePortQuest"},
    {Level = 1525, Name = "Pistol Billionaire", Quest = "PiratePortQuest"},
    {Level = 2500, Name = "Sun-kissed Warrior", Quest = "TikiQuest2"},
}

function Farm:GetCurrentMob()
    local level = Player.Data.Level.Value
    local currentMob = self.MobList[1]
    
    for _, mob in ipairs(self.MobList) do
        if level >= mob.Level then
            currentMob = mob
        else
            break
        end
    end
    
    return currentMob
end

function Farm:ActivateHaki()
    if not self.Config.AutoHaki then return end
    
    if not Player.Character:FindFirstChild("HasBuso") then
        pcall(function()
            local args = {[1] = "Buso"}
            game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
        end)
    end
end

function Farm:FastAttack()
    if not self.Config.FastAttack then return end
    
    pcall(function()
        local combat = ReplicatedStorage:FindFirstChild("Combat")
        if combat then
            -- ImplementaÃ§Ã£o de fast attack
            game:GetService("VirtualUser"):CaptureController()
            game:GetService("VirtualUser"):Button1Down(Vector2.new(1, 1))
        end
    end)
end

function Farm:FarmMob(mobData)
    local mobName = mobData.Name
    local enemies = workspace.Enemies:GetChildren()
    
    for _, enemy in pairs(enemies) do
        if enemy.Name == mobName and enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
            repeat
                task.wait()
                
                if not enemy or not enemy.Parent or enemy.Humanoid.Health <= 0 then
                    break
                end
                
                if enemy:FindFirstChild("HumanoidRootPart") and Player.Character and Player.Character:FindFirstChild("HumanoidRootPart") then
                    local enemyPos = enemy.HumanoidRootPart.Position
                    local distance = (Player.Character.HumanoidRootPart.Position - enemyPos).Magnitude
                    
                    if distance > self.Config.AttackDistance then
                        Player.Character.HumanoidRootPart.CFrame = CFrame.new(
                            enemyPos.X,
                            enemyPos.Y + 10,
                            enemyPos.Z
                        )
                    end
                    
                    self:ActivateHaki()
                    self:FastAttack()
                    
                    self.Status.CurrentMob = mobName
                    self.Status.CurrentTask = "Farmando: " .. mobName
                end
                
                AntiBan:Wait(0.1, 0.3)
                
            until not self.Config.Enabled or enemy.Humanoid.Health <= 0
        end
    end
end

function Farm:StartLevelFarm()
    spawn(function()
        while self.Config.Enabled and self.Config.FarmLevel do
            task.wait(0.5)
            
            self.Status.Level = Player.Data.Level.Value
            local mobData = self:GetCurrentMob()
            
            self:FarmMob(mobData)
            
            AntiBan:Wait(1, 2)
        end
    end)
end

function Farm:OptimizeFPS()
    if not self.Config.BoostFPS then return end
    
    pcall(function()
        if self.Config.DisableRendering then
            for _, v in pairs(workspace:GetDescendants()) do
                if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then
                    v.Material = Enum.Material.SmoothPlastic
                    v.Reflectance = 0
                end
            end
        end
        
        if self.Config.RemoveEffects then
            for _, v in pairs(game:GetService("Lighting"):GetChildren()) do
                if v:IsA("PostEffect") or v:IsA("BlurEffect") or v:IsA("BloomEffect") then
                    v.Enabled = false
                end
            end
        end
        
        settings().Rendering.QualityLevel = Enum.QualityLevel.Level01
    end)
    
    print("âš¡ OtimizaÃ§Ã£o de FPS ativada")
end

function Farm:Initialize()
    self:OptimizeFPS()
    print("ðŸ”¥ Sistema de Farm LoboFire iniciado!")
end

