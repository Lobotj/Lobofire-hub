local UI = {}
UI.__index = UI

local CoreGui = game:GetService("CoreGui")
local TweenService = game:GetService("TweenService")

UI.Config = {
    Theme = {
        Primary = Color3.fromRGB(255, 85, 0),
        Secondary = Color3.fromRGB(255, 140, 0),
        Background = Color3.fromRGB(20, 20, 25),
        BackgroundDark = Color3.fromRGB(15, 15, 20),
        Text = Color3.fromRGB(255, 255, 255),
        TextDim = Color3.fromRGB(180, 180, 180),
        Success = Color3.fromRGB(0, 255, 100),
    },
}

function UI:CreateButton(parent, text, position, size, callback)
    local button = Instance.new("TextButton")
    button.Name = text
    button.Parent = parent
    button.BackgroundColor3 = self.Config.Theme.Primary
    button.BorderSizePixel = 0
    button.Position = position
    button.Size = size
    button.Font = Enum.Font.GothamBold
    button.Text = text
    button.TextColor3 = self.Config.Theme.Text
    button.TextSize = 14
    button.AutoButtonColor = false
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = button
    
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {
            BackgroundColor3 = self.Config.Theme.Secondary
        }):Play()
    end)
    
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.2), {
            BackgroundColor3 = self.Config.Theme.Primary
        }):Play()
    end)
    
    if callback then
        button.MouseButton1Click:Connect(callback)
    end
    
    return button
end

function UI:CreateStatusLabel(parent, labelText, position, size)
    local container = Instance.new("Frame")
    container.Name = labelText .. "Container"
    container.Parent = parent
    container.BackgroundColor3 = self.Config.Theme.BackgroundDark
    container.BorderSizePixel = 0
    container.Position = position
    container.Size = size
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 6)
    corner.Parent = container
    
    local label = Instance.new("TextLabel")
    label.Name = "Label"
    label.Parent = container
    label.BackgroundTransparency = 1
    label.Position = UDim2.new(0, 10, 0, 0)
    label.Size = UDim2.new(0.4, -10, 1, 0)
    label.Font = Enum.Font.GothamBold
    label.Text = labelText
    label.TextColor3 = self.Config.Theme.TextDim
    label.TextSize = 12
    label.TextXAlignment = Enum.TextXAlignment.Left
    
    local value = Instance.new("TextLabel")
    value.Name = "Value"
    value.Parent = container
    value.BackgroundTransparency = 1
    value.Position = UDim2.new(0.4, 0, 0, 0)
    value.Size = UDim2.new(0.6, -10, 1, 0)
    value.Font = Enum.Font.Gotham
    value.Text = "..."
    value.TextColor3 = self.Config.Theme.Text
    value.TextSize = 12
    value.TextXAlignment = Enum.TextXAlignment.Right
    
    return container, value
end

