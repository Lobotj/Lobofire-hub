print("â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•")

local LBHub = {}
LBHub.Version = "2.0"
LBHub.LoadTime = tick()

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- CONFIGURAÃ‡Ã•ES
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

LBHub.Config = {
    AutoFarm = true,
    AutoFarmLevel = true,
    AutoFarmMastery = true,
    AutoFarmBoss = true,
    AutoSeaEvent = true,
    
    FarmFruitMastery = true,
    FarmWeaponMastery = true,
    
    AutoCollectFruit = true,
    
    -- Anti-Ban
    UseAntiBan = true,
    HumanizedMovement = true,
    RandomDelays = true,
    MinDelay = 0.15,
    MaxDelay = 0.35,
    
    -- Performance
    FastMode = true,
    
    -- Security
    AntiKick = true,
    
    Notifications = true,
}

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- SERVIÃ‡OS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local VirtualUser = game:GetService("VirtualUser")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")

local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local Humanoid = Character:WaitForChild("Humanoid")
local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")

Player.CharacterAdded:Connect(function(char)
    Character = char
    Humanoid = char:WaitForChild("Humanoid")
    HumanoidRootPart = char:WaitForChild("HumanoidRootPart")
end)

LBHub.Stats = {
    QuestsCompleted = 0,
    BossesDefeated = 0,
    FruitsCollected = 0,
    Runtime = 0,
    StartTime = tick()
}

LBHub.CurrentTask = "Aguardando inÃ­cio..."
LBHub.IsRunning = false

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- FUNÃ‡Ã•ES ÃšTEIS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function LBHub:Notify(title, text, duration)
    if not LBHub.Config.Notifications then return end
    pcall(function()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "ğŸº LB Hub | " .. title,
            Text = text,
            Duration = duration or 5,
        })
    end)
end

function LBHub:GetDistance(pos)
    if not HumanoidRootPart then return math.huge end
    return (HumanoidRootPart.Position - pos).Magnitude
end

function LBHub:TweenTo(targetCFrame)
    if not HumanoidRootPart then return end
    
    local distance = self:GetDistance(targetCFrame.Position)
    local speed = LBHub.Config.FastMode and 350 or 250
    
    local tween = TweenService:Create(
        HumanoidRootPart,
        TweenInfo.new(distance / speed, Enum.EasingStyle.Linear),
        {CFrame = targetCFrame}
    )
    
    tween:Play()
    return tween
end

function LBHub:EquipWeapon(weaponName)
    pcall(function()
        local tool = Player.Backpack:FindFirstChild(weaponName)
        if tool then
            Humanoid:EquipTool(tool)
        end
    end)
end

function LBHub:GetEnemies()
    local enemies = {}
    pcall(function()
        for _, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
            if v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 then
                table.insert(enemies, v)
            end
        end
    end)
    return enemies
end

function LBHub:GetNearestEnemy()
    local nearestEnemy = nil
    local shortestDistance = math.huge
    
    for _, enemy in pairs(self:GetEnemies()) do
        local distance = self:GetDistance(enemy.HumanoidRootPart.Position)
        if distance < shortestDistance then
            shortestDistance = distance
            nearestEnemy = enemy
        end
    end
    
    return nearestEnemy
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- ANTI-BAN SYSTEM
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

LBHub.AntiBan = {}

function LBHub.AntiBan:GetRandomDelay()
    return math.random(LBHub.Config.MinDelay * 100, LBHub.Config.MaxDelay * 100) / 100
end

function LBHub.AntiBan:Wait()
    if LBHub.Config.UseAntiBan and LBHub.Config.RandomDelays then
        wait(self:GetRandomDelay())
    end
end

function LBHub.AntiBan:AntiKick()
    Player.Idled:Connect(function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end)
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- QUEST SYSTEM
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

LBHub.Quest = {}

function LBHub.Quest:GetCurrentQuest()
    local quests = {
        {Level = 1, Name = "Bandit", QuestGiver = "BanditQuest1", Number = 1},
        {Level = 10, Name = "Monkey", QuestGiver = "JungleQuest", Number = 1},
        {Level = 15, Name = "Gorilla", QuestGiver = "JungleQuest", Number = 2},
        {Level = 30, Name = "Brute", QuestGiver = "BuggyQuest1", Number = 2},
        {Level = 60, Name = "Desert Officer", QuestGiver = "DesertQuest", Number = 2},
        {Level = 75, Name = "Snow Bandit", QuestGiver = "SnowQuest", Number = 1},
        {Level = 90, Name = "Snowman", QuestGiver = "SnowQuest", Number = 2},
        {Level = 100, Name = "Chief Petty Officer", QuestGiver = "MarineQuest2", Number = 2},
        {Level = 120, Name = "Sky Bandit", QuestGiver = "SkyQuest", Number = 1},
        {Level = 150, Name = "Dark Master", QuestGiver = "SkyQuest", Number = 2},
        {Level = 700, Name = "Raider", QuestGiver = "Area1Quest", Number = 1},
        {Level = 850, Name = "Swan Pirate", QuestGiver = "Area2Quest", Number = 1},
        {Level = 950, Name = "Marine Commodore", QuestGiver = "MarineQuest3", Number = 2},
    }
    
    local playerLevel = Player.Data.Level.Value
    local selectedQuest = quests[1]
    
    for _, quest in pairs(quests) do
        if playerLevel >= quest.Level then
            selectedQuest = quest
        end
    end
    
    return selectedQuest
end

function LBHub.Quest:Take()
    pcall(function()
        local quest = self:GetCurrentQuest()
        local args = {
            [1] = "StartQuest",
            [2] = quest.QuestGiver,
            [3] = quest.Number
        }
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    end)
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- AUTO FARM PRINCIPAL
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function LBHub:AutoFarm()
    spawn(function()
        while LBHub.IsRunning do
            pcall(function()
                LBHub.CurrentTask = "Farmando Level"
                
                -- Pegar quest
                if Player.PlayerGui.Main.Quest.Visible == false then
                    LBHub.Quest:Take()
                    wait(1)
                end
                
                -- Farmar inimigos
                local enemy = self:GetNearestEnemy()
                if enemy and enemy:FindFirstChild("HumanoidRootPart") then
                    repeat
                        wait()
                        
                        -- Movimento com offset humanizado
                        local offset = LBHub.Config.HumanizedMovement and 
                            Vector3.new(math.random(-5,5), math.random(10,20), math.random(-5,5)) or 
                            Vector3.new(0, 15, 0)
                        
                        self:TweenTo(enemy.HumanoidRootPart.CFrame * CFrame.new(offset))
                        
                        -- Equipar arma
                        local weapon = Player.Backpack:FindFirstChildOfClass("Tool")
                        if weapon then
                            self:EquipWeapon(weapon.Name)
                        end
                        
                        -- Atacar
                        game:GetService("VirtualUser"):CaptureController()
                        game:GetService("VirtualUser"):Button1Down(Vector2.new(1,1))
                        
                        LBHub.AntiBan:Wait()
                        
                    until not enemy or enemy.Humanoid.Health <= 0 or not LBHub.IsRunning
                    
                    LBHub.Stats.QuestsCompleted = LBHub.Stats.QuestsCompleted + 1
                end
            end)
            wait(0.5)
        end
    end)
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- AUTO BOSS FARM
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function LBHub:FarmBoss()
    spawn(function()
        while LBHub.IsRunning do
            pcall(function()
                local bosses = {
                    "Saber Expert", "The Gorilla King", "Bobby", "Yeti",
                    "Mob Leader", "Vice Admiral", "Warden", "Chief Warden",
                    "Swan", "rip_indra True Form", "Cake Queen"
                }
                
                for _, bossName in pairs(bosses) do
                    local boss = game.Workspace.Enemies:FindFirstChild(bossName)
                    if boss and boss:FindFirstChild("Humanoid") and boss.Humanoid.Health > 0 then
                        LBHub.CurrentTask = "Boss: " .. bossName
                        
                        repeat
                            wait()
                            self:TweenTo(boss.HumanoidRootPart.CFrame * CFrame.new(0, 30, 0))
                            
                            local weapon = Player.Backpack:FindFirstChildOfClass("Tool")
                            if weapon then self:EquipWeapon(weapon.Name) end
                            
                            game:GetService("VirtualUser"):Button1Down(Vector2.new(1,1))
                            
                        until not boss or boss.Humanoid.Health <= 0 or not LBHub.IsRunning
                        
                        LBHub.Stats.BossesDefeated = LBHub.Stats.BossesDefeated + 1
                        self:Notify("Boss!", bossName .. " derrotado!", 3)
                    end
                end
            end)
            wait(3)
        end
    end)
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- AUTO COLLECT FRUITS
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function LBHub:CollectFruits()
    spawn(function()
        while LBHub.IsRunning do
            pcall(function()
                for _, v in pairs(game.Workspace:GetChildren()) do
                    if string.find(v.Name, "Fruit") and v:FindFirstChild("Handle") then
                        LBHub.CurrentTask = "Coletando: " .. v.Name
                        self:TweenTo(v.Handle.CFrame)
                        wait(1)
                        LBHub.Stats.FruitsCollected = LBHub.Stats.FruitsCollected + 1
                        self:Notify("Fruta!", v.Name .. " coletada!", 2)
                    end
                end
            end)
            wait(10)
        end
    end)
end

-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
-- GUI INTERFACE
-- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•

function LBHub:CreateGUI()
